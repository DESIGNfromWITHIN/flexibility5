        'object' => '{"id":15,"source":0,"property_preprocess":0,"name":"migxJsonToPlaceholders","description":"","editor_type":0,"category":0,"cache_type":0,"snippet":"$value = $modx->getOption(\'value\',$scriptProperties,\'\');\\n$prefix = $modx->getOption(\'prefix\',$scriptProperties,\'\');\\n\\n\\/\\/$modx->setPlaceholders($modx->fromJson($value),$prefix,\'\',true);\\n\\n$values = $modx->fromJson($value);\\nif (is_array($values)){\\n    foreach ($values as $key => $value){\\n        $value = $value == null ? \'\' : $value;\\n        $modx->setPlaceholder($prefix . $key, $value);\\n    }\\n}","locked":0,"properties":null,"moduleguid":"","static":0,"static_file":"","content":"$value = $modx->getOption(\'value\',$scriptProperties,\'\');\\n$prefix = $modx->getOption(\'prefix\',$scriptProperties,\'\');\\n\\n\\/\\/$modx->setPlaceholders($modx->fromJson($value),$prefix,\'\',true);\\n\\n$values = $modx->fromJson($value);\\nif (is_array($values)){\\n    foreach ($values as $key => $value){\\n        $value = $value == null ? \'\' : $value;\\n        $modx->setPlaceholder($prefix . $key, $value);\\n    }\\n}"}',
